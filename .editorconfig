[*]
tab_width = 2
ij_any_array_initializer_new_line_after_left_brace = true
ij_any_array_initializer_right_brace_on_new_line = true
ij_javascript_spaces_within_object_literal_braces = true
ij_coffeescript_spaces_within_object_braces = true
ij_javascript_spaces_within_object_type_braces = true
ij_any_blank_lines_after_imports = 1
ij_javascript_import_merge_members = true
ij_javascript_import_sort_members = true
ij_javascript_import_sort_module_name = true
ij_typescript_import_merge_members = true
ij_typescript_import_sort_members = true
ij_typescript_import_sort_module_name = true
trim_trailing_whitespace = true
charset = utf-8
indent_size = 2
indent_style = space
end_of_line = lf
max_line_length = off
insert_final_newline = false
ij_any_space_after_colon = true
ij_any_space_after_comma = true
